void keyPressed() {
  if (key == CODED) {
    if (keyCode == LEFT) {
      if(lastplatx > 0){
        spritetype.add(0);
        x.add(lastplatx - 48);
        y.add(lastplaty);
        lastplatx -= 48;
      }
    }
    if(keyCode == RIGHT){
      if(lastplatx < 1000){
        spritetype.add(0);
        x.add(lastplatx + 48);
        y.add(lastplaty);
        lastplatx += 48;
      }
    }
    if(keyCode == UP){
      if(lastplaty > 0){
        spritetype.add(0);
        x.add(lastplatx);
        y.add(lastplaty - 48);
        lastplaty -= 48;
      }
    }
    if(keyCode == DOWN){
      if(lastplaty + 48 < 800){
        spritetype.add(0);
        x.add(lastplatx);
        y.add(lastplaty + 48);
        lastplaty += 48;
      }
    }
  }
  if(keyPressed){
    if(key == 'e' || key == 'E'){
      print("\n{");
      for(int i = 0; i < x.size(); i++){
        print(spritetype.get(i) + ", " + x.get(i) + ", " + y.get(i));
        if(i == x.size() - 1)
          print("}");
        else
          print(", ");
      }
    }else if(key == ','){//left
      selecteditem--;
      if(selecteditem < 0)
        selecteditem = numofitems;
    }else if(key == '.'){//right
      selecteditem++;
      if(selecteditem > numofitems)
        selecteditem = 0;
    }else if(key == 'd' || key == 'D'){
      spritetype.clear();
      x.clear();
      y.clear();
    }
  }
}

